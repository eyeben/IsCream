    "import PyPDF2\n",
    "\n",
    "# PDF 파일 읽기\n",
    "pdf_file = \"htp2.pdf\"\n",
    "with open(pdf_file, \"rb\") as file:\n",
    "    reader = PyPDF2.PdfReader(file)\n",
    "    text_data = [page.extract_text() for page in reader.pages]\n",
    "\n",
    "# 텍스트 데이터 리스트 출력\n",
    "print(text_data)"
   ],
